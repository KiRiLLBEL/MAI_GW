{
  "name": "arch-structurizr",
  "displayName": "Arch-Structurizr",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "arch",
        "aliases": [
          "Arch",
          "arch"
        ],
        "extensions": [
          ".arch"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "configurationDefaults": {
      "[arch]": {
        "editor.semanticHighlighting.enabled": true
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "typescript": "^4.8.0",
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "path": "^0.12.7",
    "vscode": "^1.1.34",
    "vscode-languageclient": "^9.0.1",
    "vscode-test": "^1.6.1"
  }
}
